<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<androidx.cardview.widget.CardView
		android:id="@+id/card"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:cardElevation="10dp"
		app:cardCornerRadius="20dp">
		<LinearLayout
			android:id="@+id/bg"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview3"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="ایجاد پروژه vsM یکی از عمل کرد ها رو انتخاب کنید"
					android:textSize="14sp"
					android:textColor="#2196F3" />
				<ImageView
					android:id="@+id/imageview3"
					android:focusable="false"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:padding="7dp"
					android:src="@drawable/variant"
					android:scaleType="centerInside" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/folder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview1"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="فشرده کردن پوشه/SkpL"
						android:textSize="12sp"
						android:textColor="#219EB7" />
					<ImageView
						android:id="@+id/imageview1"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="5dp"
						android:src="@drawable/projectcompers_1"
						android:scaleType="centerInside" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/file"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview2"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="فشرده کردن فایل"
						android:textSize="12sp"
						android:textColor="#219EB7" />
					<ImageView
						android:id="@+id/imageview2"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="5dp"
						android:src="@drawable/projectcompers_2"
						android:scaleType="centerInside" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/fb"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview4"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="اشتراک گذاری فایل"
					android:textSize="12sp"
					android:textColor="#00BCD4" />
				<ImageView
					android:id="@+id/imageview4"
					android:focusable="false"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:padding="5dp"
					android:src="@drawable/share"
					android:scaleType="centerInside" />
			</LinearLayout>
		</LinearLayout>
	</androidx.cardview.widget.CardView>
</LinearLayout>
